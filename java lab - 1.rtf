        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Enter a number (1-7): ");\par
        int day = scanner.nextInt();\par
\par
        switch (day) \{\par
            case 1:\par
                System.out.println("Monday");\par
                break;\par
            case 2:\par
                System.out.println("Tuesday");\par
                break;\par
            case 3:\par
                System.out.println("Wednesday");\par
                break;\par
            case 4:\par
                System.out.println("Thursday");\par
                break;\par
            case 5:\par
                System.out.println("Friday");\par
                break;\par
            case 6:\par
                System.out.println("Saturday");\par
                break;\par
            case 7:\par
                System.out.println("Sunday");\par
                break;\par
